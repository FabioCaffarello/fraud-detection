{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "production": [
      "default"
    ],
    "sharedGlobals": [
      "{workspaceRoot}/go.work",
      "{workspaceRoot}/.github/workflows/*.yml"
    ]
  },
  "nxCloudId": "67ccd44d48d441480c22ea1f",
  "plugins": [
    {
      "plugin": "@nx-go/nx-go"
    },
    {
      "plugin": "@nxlv/python",
      "options": {
        "packageManager": "uv"
      }
    }
  ]
}
